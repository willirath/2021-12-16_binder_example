   "outputs": [],
   "source": [
    "def calc_pi(n=10_000):\n",
    "    xy = np.random.uniform(0, 1, size=(n, 2))\n",
    "    r2 = (xy ** 2).sum(axis=-1)\n",
    "    pi = 4.0 * (r2 < 1).mean()\n",
    "    return pi"
   ]
  },
